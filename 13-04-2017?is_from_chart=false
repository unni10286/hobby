<script type="text/javascript">
    function returnJourney(org, dest, travel_date, is_round_trip_journey, normal_or_bima, res_arrival_datetime, reservation_id){
        if(org == ""){
            var org = $('#return_from').val();
        }
        if(dest == ""){
            var dest = $('#return_to').val();
        }
        if(travel_date == ""){
            var travel_date = $('#return_on').val();
        }
        if(is_round_trip_journey == ""){
            is_round_trip_journey = "true";
        }
        $('#departing_bp').val($('#searchbus_boarding_at').val());
        $('#departing_dp').val($('#searchbus_drop_off').val());
        $('#onward_selected_seats_span').val(spaceAftercommaforStr($("#selected_seats_span").html()));
        $('#onward_selected_boarding_name').val($('#searchbus_boarding_at option:selected').text());
        $.ajax({url: '/e_bookings/search_results_for_return',
            data: {searchbus : {from : org, to : dest, depart : travel_date, is_round_trip_journey : is_round_trip_journey}, normal_or_bima : normal_or_bima, res_arrival_datetime: res_arrival_datetime, reservation_id: reservation_id},
            type: 'post',
            success: function(result){
                /*$('#some_id').css('display', 'none');*/
                $('#return_trip').removeAttr('style');
            }
        });
    }
    function make_this_round_trip(){
        var departing_date = $("#searchbus_depart").val();
        departing_date = departing_date.split('/');
        departing_date = new Date(departing_date[2], departing_date[1] - 1, departing_date[0]);
        var returning_date = $('#searchbus_return_date').val()
        returning_date = returning_date.split('/');
        returning_date = new Date(returning_date[2], returning_date[1] - 1, returning_date[0]);
        if(departing_date > returning_date){
            alert(return_date_should_not_be_earlier_than_departing_date);
            return false;
        }
        $('#return_date_div').removeAttr('style');
        $("#searchbus_return").val($('#searchbus_return_date').val()); 
        $('#triptype_oneway').get(0).checked = false;
        $('#triptype_roundtrip').get(0).checked = true;
        $("#search-form").submit();
    }
    function get_custom_search_res_new(date, for_return, change_dates, prev_date, is_round_trip_journey){
        if(for_return == "false"){
            $("#searchbus_depart").val(date);
            $('#onward_journey_date').html(date);
            var departing_date = $("#searchbus_depart").val();
            departing_date = departing_date.split('/');
            departing_date = new Date(departing_date[2], departing_date[1] - 1, departing_date[0]);
            if ($("#show_trip_options_priv").val() == "true") {
                var returning_date = $("#searchbus_return").val();
                returning_date = returning_date.split('/');   
                returning_date = new Date(returning_date[2], returning_date[1] - 1, returning_date[0]);
                if (($("#searchbus_return").val() != undefined) && ( departing_date > returning_date)){
                    $("#searchbus_return").val(date);
                    $('#return_journey_date').html(date);
                }
            }
            var org = $('#trip_from').val();
            var dest = $('#trip_to').val();
            var url_path = '/e_bookings/search_results'
            var normal_or_bima = ''
        }
        else{
            returning_date = date.split('/');
            returning_date = new Date(returning_date[2], returning_date[1] - 1, returning_date[0]);
            onward_date = $("#searchbus_depart").val();
            onward_date = onward_date.split('/');
            onward_date = new Date(onward_date[2], onward_date[1] - 1, onward_date[0]);
            if(onward_date > returning_date){
                alert(return_date_should_not_be_earlier_than_departing_date);
                return false;
            }
            $("#searchbus_return").val(date);
            $('#return_journey_date').html(date);
            var org = $('#return_from').val();
            var dest = $('#return_to').val();
            var url_path = '/e_bookings/search_results_for_return'
            var normal_or_bima = $('#selected_service_type').val();
        }
        $('#prev_date_for_cal').val(prev_date);
        $("#render_new_dates").val(change_dates);
        $("#rountrip_return").val(for_return);

        $.ajax({url: url_path,
            data: {searchbus : {from : org, to : dest, depart : date, is_round_trip_journey : is_round_trip_journey}, normal_or_bima : normal_or_bima},
            type: 'post',
            success: function(result){
            }
        });
    }
    function skipReturnTrip(){
        departing_res_id = $('#selected_departing_id').val();
        $('#is_round_trip_journey').val(false);
        $('#is_for_return').val(false);
        if($('#selected_service_type').val() == "normal"){
            selectBookLink(departing_res_id, '', '', '', '', true);
        }
        else{
            selectBimaBookLink(departing_res_id, '', 'false', 'false', true)
        }
    }
</script>
