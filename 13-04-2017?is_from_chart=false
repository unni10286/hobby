<script type="text/javascript">
$(function() {
	var travel_max_date = new Date($("#travel_max_date").val());
    var max_month = travel_max_date.getMonth();
    var max_date = travel_max_date.getDate();
    var max_year = travel_max_date.getFullYear();
	var dates = $("#searchbus_depart, #searchbus_return").datepicker({
		defaultDate: "",
		minDate: 0,
		changeMonth: true,
		numberOfMonths: 2,
		maxDate: new Date(max_year, max_month, max_date),
		showOn: 'both', buttonImage: "//cdn-assets2-cf-india-static.ticketsimply.net/assets/calendar-4233bcc388abfc9ceda6fb89fdc51b9c.gif", dateFormat: 'dd/mm/yy', numberOfMonths: 2, firstDay: 1, buttonImageOnly: true, showAnim: 'fold', showButtonPanel: true,
		onSelect: function( selectedDate ) {
			var option = this.id == "searchbus_depart" ? "minDate" : "maxDate",
				instance = $( this ).data( "datepicker" ),
				date = $.datepicker.parseDate(
					instance.settings.dateFormat ||
					$.datepicker._defaults.dateFormat,
					selectedDate, instance.settings );
			dates.not( this ).datepicker( "option", option, date );
		}
	});
});
</script>
